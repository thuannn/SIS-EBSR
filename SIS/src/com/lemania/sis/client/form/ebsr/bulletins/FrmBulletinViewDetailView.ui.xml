<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel width="100%">
		<g:VerticalPanel width="100%">
			<g:Label text="CONSULTER LES NOTES" horizontalAlignment="ALIGN_CENTER"
				styleName="pageTitle" />
			<g:VerticalPanel width="100%" spacing="5">
				<g:HorizontalPanel spacing="5" styleName="subSection">
					<g:HorizontalPanel width="400px" spacing="2"
						horizontalAlignment="ALIGN_RIGHT" ui:field="pnlAdmin" visible="false">
						<g:Label text="Classe :" />
						<g:ListBox ui:field="lstClasses" width="300px" />
					</g:HorizontalPanel>
					<g:HorizontalPanel spacing="2" width="400px"
						horizontalAlignment="ALIGN_RIGHT">
						<g:Label text="Bulletin(s) :" />
						<g:ListBox width="300px" ui:field="lstBulletins" />
					</g:HorizontalPanel>
				</g:HorizontalPanel>

				<g:HorizontalPanel width="100%" ui:field="pnlLogo"
					verticalAlignment="ALIGN_BOTTOM">
					<g:Image url="images/logo_ebsr.png" width="" height=""
						ui:field="imgLogo" />
					<g:Label ui:field="lblBulletinTitle" styleName="fieldTitle"></g:Label>
				</g:HorizontalPanel>

				<g:FlexTable width="100%" ui:field="tblBulletin" />

			</g:VerticalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder>
